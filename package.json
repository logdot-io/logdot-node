{
  "name": "@logdot-io/sdk",
  "version": "1.0.1",
  "description": "LogDot SDK for Node.js - Cloud logging and metrics",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "logging",
    "metrics",
    "cloud",
    "monitoring",
    "logdot",
    "observability"
  ],
  "author": "LogDot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/logdot-io/logdot-node"
  },
  "homepage": "https://logdot.io",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0"
  }
}
