{
  "name": "@logdot-io/sdk",
  "version": "1.0.2",
  "description": "LogDot SDK for Node.js - Cloud logging and metrics",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs.d.ts",
      "import": "./dist/nextjs.js",
      "require": "./dist/nextjs.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "logging",
    "metrics",
    "cloud",
    "monitoring",
    "logdot",
    "observability"
  ],
  "author": "LogDot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/logdot-io/logdot-node"
  },
  "homepage": "https://logdot.io",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@opentelemetry/sdk-node": ">=0.50.0",
    "@opentelemetry/auto-instrumentations-node": ">=0.40.0",
    "@opentelemetry/sdk-trace-base": ">=1.20.0",
    "@opentelemetry/sdk-metrics": ">=1.20.0",
    "@opentelemetry/core": ">=1.20.0",
    "@opentelemetry/api": ">=1.7.0"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/sdk-node": { "optional": true },
    "@opentelemetry/auto-instrumentations-node": { "optional": true },
    "@opentelemetry/sdk-trace-base": { "optional": true },
    "@opentelemetry/sdk-metrics": { "optional": true },
    "@opentelemetry/core": { "optional": true },
    "@opentelemetry/api": { "optional": true }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "vitest": "^1.0.0",
    "@opentelemetry/sdk-node": "^0.50.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0",
    "@opentelemetry/sdk-trace-base": "^1.20.0",
    "@opentelemetry/sdk-metrics": "^1.20.0",
    "@opentelemetry/core": "^1.20.0",
    "@opentelemetry/api": "^1.7.0"
  }
}
